{
  "name": "@supernpm2024/modi-consequuntur-mollitia-nesciunt",
  "version": "1.0.0",
  "description": "ESLint plugin for Ember.js apps",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "ember",
    "ember.js",
    "plugin",
    "styleguide",
    "rules"
  ],
  "license": "MIT",
  "exports": {
    ".": "./lib/index.js",
    "./configs/*": "./lib/config/*.js"
  },
  "main": "./lib/index.js",
  "directories": {
    "test": "test",
    "rules": "rules"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 99,
        "lines": 98,
        "statements": 98
      }
    },
    "testMatch": [
      "**/tests/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/tests/helpers/"
    ]
  },
  "dependencies": {
    "@ember-data/rfc395-data": "^0.0.4",
    "@supernpm2024/cupiditate-sed-aliquam-quae": "^1.0.0",
    "@supernpm2024/legendary-broccoli": "^1.0.0",
    "@supernpm2024/modi-quod-recusandae-placeat": "^1.0.0",
    "@supernpm2024/officia-eum-pariatur-temporibus": "^1.0.0",
    "@supernpm2024/tenetur-tempore-ex-illo": "^1.0.0",
    "@supernpm2024/unde-delectus-corporis-commodi": "^1.0.0",
    "css-tree": "^2.3.1",
    "ember-eslint-parser": "^0.3.7",
    "ember-rfc176-data": "^0.3.18",
    "eslint-utils": "^3.0.0",
    "estraverse": "^5.3.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "requireindex": "^1.2.0",
    "snake-case": "^3.0.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@eslint/eslintrc": "^3.0.1",
    "@eslint/js": "^8.55.0",
    "@release-it-plugins/lerna-changelog": "^6.0.0",
    "@types/eslint": "^8.44.6",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-doc-generator": "^1.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unicorn": "^51.0.0",
    "eslint-remote-tester": "^3.0.1",
    "globals": "^14.0.0",
    "jest": "^29.7.0",
    "jquery": "^3.7.1",
    "jsdom": "^24.0.0",
    "markdownlint-cli": "^0.39.0",
    "npm-package-json-lint": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "release-it": "^17.0.0",
    "sort-package-json": "^2.6.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.0.0"
  },
  "peerDependencies": {
    "eslint": ">= 8",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": "18.* || 20.* || >= 21"
  },
  "volta": {
    "node": "21.5.0",
    "yarn": "1.22.21"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "changelog": {
    "repo": "ember-cli/eslint-plugin-ember",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    }
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/supernpm2024/modi-consequuntur-mollitia-nesciunt.git"
  },
  "homepage": "https://github.com/supernpm2024/modi-consequuntur-mollitia-nesciunt/#readme",
  "bugs": {
    "url": "https://github.com/supernpm2024/modi-consequuntur-mollitia-nesciunt/issues"
  },
  "author": "ellentea",
  "packageManager": "yarn@4.1.1"
}
